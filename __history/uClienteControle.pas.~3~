    procedure setBairro(const value: string);
    procedure setCidade(const value: string);
    procedure setEstado(const value: string);
    procedure setPais(const value: string);
  public
    property Nome:  read getNome write setNome;
    property Identidade:  read getIdentidade write setIdentidade;
    property CPF:  read getCPF write setCPF;
    property Telefone:  read getTelefone write setTelefone;
    property Email:  read getEmail write setEmail;
    property Endereco:  read getEndereco write setEndereco;
    property Cep:  read getCep write setCep;
    property Logradouro:  read getLogradouro write setLogradouro;
    property Numero:  read getNumero write setNumero;
    property Complemento:  read getComplemento write setComplemento;
    property Bairro:  read getBairro write setBairro;
    property Cidade:  read getCidade write setCidade;
    property Estado:  read getEstado write setEstado;
    property Pais:  read getPais write setPais;
  protected
end;

