begin
  Result := FEmail;
end;

function TClienteControle.getEndereco: string;
begin
  Result := FEndereco;
end;

function TClienteControle.getEstado: string;
begin
  Result := FEstado;
end;

function TClienteControle.getIdentidade: string;
begin
  Result := FIdentidade;
end;

function TClienteControle.getLogradouro: string;
begin
  Result := FLogradouro;
end;

function TClienteControle.getNome: string;
begin
  Result := FNome;
end;

function TClienteControle.getNumero: string;
begin
  Result := FNumero;
end;

function TClienteControle.getPais: string;
begin
  Result := FPais;
end;

function TClienteControle.getTelefone: string;
begin
  Result := FTelefone;
end;

procedure TClienteControle.setBairro(const value: string);
begin
  FBairro := value;
end;

procedure TClienteControle.setCep(const value: string);
begin
  FCep := value;
end;

procedure TClienteControle.setCidade(const value: string);
begin
  FCidade := value;
end;

procedure TClienteControle.setComplemento(const value: string);
begin
  FComplemento := value;
end;

procedure TClienteControle.setCPF(const value: string);
begin
  FCPF := value;
end;

procedure TClienteControle.setEmail(const value: string);
begin
  FEmail := value;
end;

procedure TClienteControle.setEndereco(const value: string);
begin
  FEndereco := value;
end;

procedure TClienteControle.setEstado(const value: string);
begin
  FEstado := value;
end;

procedure TClienteControle.setIdentidade(const value: string);
begin
  FIdentidade := value;
end;

procedure TClienteControle.setLogradouro(const value: string);
begin
  FLogradouro := value;
end;

procedure TClienteControle.setNome(const value: string);
begin
  FNome := value;
end;

procedure TClienteControle.setNumero(const value: string);
begin
  FNumero := value;
end;

procedure TClienteControle.setPais(const value: string);
begin
  FPais := value;
end;

